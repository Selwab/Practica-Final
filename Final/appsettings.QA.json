{
    "Title": "Practica en QA",
    "Serilog": {
        "WriteTo": [
          {
            "Name": "File",
            "Args": {
              "path": "logs/log-.txt",
              "rollingInterval": "Day",
              "retainedFileCountLimit": 7
            }
          }
        ],
        "MinimumLevel": {
          "Default": "Warning",
          "Override": {
            "Microsoft": "Information",
            "System": "Information"
          }
        }
      }
}
  